@charset "utf-8";
.container{
	max-width:640px;
	width:100%;
	margin:auto;
	background-color:#f6f6f6;
}
/* ¶¥²¿start */
.c-head{
	position:fixed;
	top:0;
	max-width:640px;
	width:100%;
	height:52px;
	margin:auto;
	background-color:rgba(0,0,0,.2);
	z-index:998;
}
.c-head .c-h-menu{
	position:absolute;
	top:18px;
	left:10px;
}
.c-head .c-h-menu i{
	display:inline-block;
	width:1.25rem;
	height:1rem;
	background:url("../images/1.png") no-repeat center/cover;
}
.c-head .c-h-search{
	position:relative;
	width: calc(100% - 80px);
	height:30px;
	margin:10px auto 0;
	text-align:right;
	line-height:30px;
	background-color:#fff;
	border-radius:20px;
}
.c-head .c-h-search .c-h-s-logo{
	position:absolute;
	top:5px;
	left:14px;
	width:30px;
	height:20px;
	background:url("../images/2.png") no-repeat center/contain;
}
.c-head .c-h-search .c-h-s-logo:after{
	position:absolute;
	top:-1px;
	right:-8px;
	content:'';
	width:1px;
	height:24px;
	background-color:#f0f0f0;
}
.c-head .c-h-search .c-h-s-glass{
	position:absolute;
	top:5px;
	left:56px;
	width:20px;
	height:20px;
	background:url("../images/3.png")no-repeat center/contain;
}
.c-head .c-h-search .c-h-s-input{
	width:calc(100% - 84px);
	height:30px;
	border:none;
	outline:none;
	border-radius:20px;
}
.c-head .c-h-login{
	position:absolute;
	top:16px;
	right:8px;
	color:#fff;
}
/* ¶¥²¿end */
/* banner start */
.banner{
	position:relative;
	width:100%;
	overflow:hidden;
}
.banner .b-carousel{
	width:1000%;
}
.banner .b-carousel li{
	float:left;
	width:10%;
}
.banner .b-carousel li img{
	width:100%;
}
.banner .b-tab{
	position:absolute;
	bottom:23px;
	left:50%;
	transform:translateX(-50%);
}
.banner .b-tab li{
	float:left;
	box-sizing:border-box;
	width:6px;
	height:6px;
	border-radius:50%;
	margin:0 4px;
	border:1px solid#fff;
}
.banner .b-tab li.active{
	background-color:#fff;
}
/* banner end */
/* advertising start */
.advertising{
	position:relative;
	width:100%;
	margin-top:-16px;
	border-radius:10px 10px 0px 0px;
}
.advertising img{
	display:block;
	width:100%;
	border-radius:10px 10px 0px 0px;
}
/* advertising end */
/* nav start*/
.nav{
	display:flex;
	flex-flow:row wrap;
	padding-bottom:20px;
	background-color:#fff;
}
.nav .n-item {
	width:20%;
}
.nav .n-item a{
	display:inline-block;
	width:100%;
	height:100%;
	font-size:12px;
	text-align:center;
	color:#686868;
}
.nav .n-item img{
	display:block;
	width:40px;
	height:40px;
	margin:10px auto 4px;
}
/* nav end*/
/* news start */
.news{
	position:relative;
	display:flex;
	width:calc(100% - 16px);
	height:30px;
	margin:0 auto 8px;
	border-radius:10px;
	background-color:#fff;
	align-items:center;
}
.news .ne-img{
	width:73px;
	height:16px;
	padding-left:8px;
}
.news .ne-img img{
	width:100%;
}
.news .ne-scroll{
	flex:5;
	height:30px;
	overflow:hidden;
}
.news .ne-scroll ul{
	animation:run 10s linear infinite;
}
.news .ne-scroll ul li{
	line-height:30px;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.news .ne-scroll ul li a{
	display:inline-block;
	width:100%;
	color:#181818;
	font-size:12px;
}
.news .ne-scroll ul li a .ne-s-hot{
	color:#f23030;
	padding:0 6px 0 8px;
}
.news .ne-more{
	flex:1;
	font-size:12px;
	text-align:center;
	border-left:1px solid#ececec;
}
.news .ne-more a{
	color:#181818;
}
@keyframes run{
	0%{transform:translateY(0);}
	0%,23%{transform:translateY(0%);}
	25%{transform:translateY(-20%);}
	25%,48%{transform:translateY(-20%);}
	50%{transform:translateY(-40%);}
	50%,73%{transform:translateY(-40%);}
	75%{transform:translateY(-60%);}
	75%,98%{transform:translateY(-60%);}
	100%{transform:translateY(-80%);}
}
/* news end */
/* welfare start */
.welfare{
	width:100%;
	margin-bottom:8px;
}
.welfare img{
	width:100%;
	border-radius:40px;
}
/* welfare end */
/* second start */
.second{
	width:100%;
	background-color:#fff;
}
.second .s-title{
	height:34px;
	line-height:34px;
}
.second .s-title .s-t-left{
	float:left;
}
.second .s-title .s-t-left a{
	color:#232326;
}
.second .s-title .s-t-left a .s-t-l-name{
	color: #f02b2b;
	margin:0 0.375rem 0;
	font-weight:700;
}
.second .s-title .s-t-left a .s-t-l-time{
	font-weight:700;
}
.second .s-title .s-t-left a .s-t-l-hout{
	padding:1px;
	border:1px solid #efefef;
}
.second .s-title .s-t-right{
	float:right;
}
.second .s-title .s-t-right a{
	color:#f23030;
	line-height:35px;
	font-size:12px;
	padding-right:10px;
}
.second .s-title .s-t-right a i{
	display:inline-block;
	width:14px;
	height:14px;
	background:url("../images/bulletin/arrow_rt.png") no-repeat center/contain;
	vertical-align:-3px;
}
.second .s-content{
	width:100%;
	overflow:hidden;
	
}
.second .s-content ul{
	width:1000%;
}
.second .s-content ul li{
	float:left;
	width:70px;
	text-align:center;
}
.second .s-content ul li .s-c-img{
	width:calc(100% - 0.3125rem);
	margin:auto;
}
.second .s-content ul li .s-c-price{
	margin-top:10px;
	color:#f23030;
	font-size:16px;
	font-weight:700;
}
.second .s-content ul li .s-c-del{
	display:inline-block;
	margin:4px 0 10px;
	color:#686868;
	font-size:12px;
}
/* second end */
/* show start */
.show{
	width:100%;
}
.show img{
	width:100%;
}
/* show end */
/* seckill start */
.seckill{
	display:flex;
	flex-flow:row wrap;
}
.seckill .s-item{
	width:25%;
}
.seckill .s-item img{
	width:100%;
}
/* seckill end */
/* graphic start */
.graphic{
	display:flex;
	flex-flow:row wrap;
	background-color:#fff;
}
.graphic .g-item{
	width:25%;
	box-sizing:border-box;
	padding:.4063rem 0 .3125rem .625rem;
	border-bottom:1px solid#f5f5f5;
}
.bd-r{
	border-right:1px solid#f5f5f5;
}
.graphic .g-item .g-i-title{
	display:inline-block;
	max-width:100%;
	background:-webkit-linear-gradient(left, rgb(254, 133, 55), rgb(240, 43, 43));
	-webkit-background-clip:text;
	-webkit-text-fill-color:transparent;
}
.graphic .g-item .g-i-describe{
	color:#22222;
	font-size:12px;
}
.graphic .g-item a img{
	width: 4.0625rem;
    height: 4.0625rem;
    margin: .3438rem .25rem 0;
}
/* graphic end */
/* floor start */
.floor{
	width:100%;
}
.floor .f-header img{
	width:100%;
}
.floor .f-c-one,
.floor .f-c-two{
	display:flex;
	flex-flow:row wrap;
}
.floor .f-c-half{
	position:relative;
	width:50%;
	padding-left:7px;
	background-color:#fff;
	box-sizing:border-box;
	border-bottom:1px solid #f5f5f5;
}
.floor .f-c-title{
	font-size:14px;
	color:#000;
	text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.floor .f-c-describe{
	font-size:12px;
	text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.life .f-c-describe{
	color:#FF5959;
}
.mall .f-c-describe{
	color:#D663E1;
}
.floor .f-c-half .f-c-down{
	overflow:hidden;
	padding:0 10px;
}
.floor .f-c-half .f-c-down img{
	float:left;
	width:55px;
	height:55px;
	margin:4px 4px 0;
}
.floor .floorBtn{
	position:absolute;
	bottom:0;
	left:0;
	background:linear-gradient(to right, rgb(255, 110, 0), rgb(255, 86, 68), rgb(255, 74, 125));
	color:#fff;
	font-size:12px;
}
.floor .floorBtn i{
	display:inline-block;
	width:8px;
	height:8px;
	margin:0 4px;
	background:url("../images/pBtn.png") no-repeat center/contain;
}
.floor .f-c-quarter{
	position:relative;
	width:25%;
	padding:5px 0 4px 8px;
	background-color:#fff;
	box-sizing:border-box;
}
.floor .f-c-quarter img{
	width: 4.0625rem;
    height: 4.0625rem;
    margin: .3438rem .25rem 0;
}
.floor .f-c-half .f-c-leftRight{
	overflow:hidden;
	padding:5px 0 4px;
}
.floor .f-c-half .f-c-leftRight .f-c-left{
	float:left;
	width:55%;
}
.floor .f-c-half .f-c-leftRight .f-c-right{
	float:left;
	width:45%;
	padding:7px 4px 0 0;
	box-sizing:border-box;
}
.floor .f-c-half .f-c-leftRight .f-c-right img{
	width:100%;
}
/* floor end */
/* recommended start */
.recommended{
	width:100%;
	background-color:#f0f2f5;
	margin-top:8px;
}
.recommended .r-header{
	position:relative;
	padding:4px;
	margin:14px 5px;
	text-align:center;
}
.recommended .r-header span{
	position:relative;
	padding:0 10px;
	background-color:#f0f2f5;
	z-index:5;
}
.recommended .r-header span i{
	display:inline-block;
	width:16px;
	height:16px;
	vertical-align:middle;
	margin-right:10px;
	background:url("../images/tuijian.png") no-repeat center/contain;
}
.recommended .r-header:after{
	position:absolute;
	top:50%;
	left:0;
	content:'';
	width:100%;
	height:1px;
	background-color:#cbcbcb;
}
.recommended .r-login{
	display:block;
	padding:3px 3px 14px;
	color:#f23030;
	font-size:14px;
	text-align:center;
}
.recommended .r-content{
	display:flex;
	flex-flow:row wrap;
	justify-content:space-between;
}
.recommended .r-c-item{
	width:49.5%;
	margin-bottom:4px;
	padding-bottom:6px;
	background-color:#fff;
}
.recommended .r-c-item img{
	width:100%;
}
.recommended .r-c-item .r-c-title{
	height:33px;
	margin:5px 0 4px;
	padding:0 4px;
	font-size:12px;
	overflow:hidden;
	text-overflow:ellipsis;
	display:-webkit-box;
	-webkit-line-clamp: 2;
	word-break:break-word;
	-webkit-box-orient:vertical;
}
.recommended .r-c-item .r-c-price{
	font-size:0px;
	color:#f23030;
	padding:0 5px;
}
.recommended .r-c-item .r-c-price .r-c-style{
	font-size:13px;
	margin-right:4px;
}
.recommended .r-c-item .r-c-price .r-c-num{
	font-size:16px;
}
.recommended .r-c-item .r-c-smallprcie{
	font-size:12px;
	margin-right:4px;
}
.recommended .r-c-item .r-c-iconC{
	background-color:#f23030;
	color:#fff;
	font-size:12px;
	padding:1px 2px;
	border-radius:2px;
}
.recommended .r-c-item .r-c-fun{
	padding:0 4px;
	height:22px;
}
.recommended .r-c-item .r-c-fun .r-c-f-praise{
	color:#999;
	font-size:12px;
	vertical-align:-4px;
}
.recommended .r-c-item .r-c-fun .r-c-f-similar{
	float:right;
	color:#686868;
	border:1px solid #bfbfbf;
	font-size:12px;
	padding:2px 4px;
}
.loading{
	height:32px;
	line-height:32px;
	text-align:center;
	border-width:1px 0 1px 0;
	border-style:solid;
	border-color:#bfbfbf;
}
/* recommended end */
/* footer start */
.footer{
	padding-bottom:54px;
	background-color:#fff;
}
.footer .f-link{
	padding:4vw 5vw;
	overflow:hidden;
	border-bottom:1px solid#bfbfbf;
}
.footer .f-link li{
	float:left;
	width:25%;
	text-align:center;
	box-sizing:border-box;
	padding:0 5px;
}

.footer .f-link li a{
	display:block;
	position:relative;
	color:#848689;
}
.footer .f-link li a:after{
	content:'';
	position:absolute;
	right:0;
	height:70%;
	width:1px;
	background-color:#d7d7d7;
	top:15%;
}
.footer .f-link li:last-child a:after{
	width:0;
}
.footer .f-icons{
	display:flex;
	flex-flow:row nowrap;
	border-bottom:1px solid#bfbfbf;
}
.footer .f-i-item{
	flex:1;
}
.footer .f-i-item img{
	width:16vw;
	max-width:4rem;
	margin:auto;
}
.footer .f-copyright{
	color:#848689;
	text-align:center;
	font-size:1.8vw;
	padding:2vw 0;
}
/* footer end */
/* footnav start */
.footNav{
	position:fixed;
	bottom:0;
	max-width:630px;
	width:calc(100% - 20px);
	height:13vw;
	max-height:50px;
	background-color:#fff;
	box-shadow:0 0 10px #bfbfbf;
	z-index:998;
}
.footNav .f-item{
	float:left;
	width:20%;
	height:100%;
	overflow:hidden;
}
.footNav .f-item img{
	width:16vw;
	max-width:4rem;
	margin:auto;
}
.footNav .f-middle{
	position:absolute;
	top:-10px;
	left:50%;
	transform:translateX(-50%);
}
/* footnav end */